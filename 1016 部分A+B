#include<stdio.h>
#include<string.h>
int main()
{
	char A[10],a,B[10],b;
	int i,cnt1=0,cnt2=0,a1,b1,suma=0,sumb=0;
	scanf("%s %c %s %c",A,&a,B,&b);
	for(i=0;i<10;i++)
	{
		if(A[i]==a)
		cnt1++;
		if(B[i]==b)
		cnt2++;
	}
	a1=a-'0';
	b1=b-'0';
	for(i=0;i<cnt1;i++)
	{
		suma=suma*10+a1;
	}
	for(i=0;i<cnt2;i++)
	{
		sumb=sumb*10+b1;
	}
	printf("%d",suma+sumb);
 }   
